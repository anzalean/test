mkdir -p sports-betting-site
cd sports-betting-site
npm init -y
npm install normalize.css live-server
mkdir -p src/assets src/styles src/js
touch src/index.html src/styles/main.css src/js/api.js src/js/main.js src/js/utils.js
mkdir -p src/assets/images src/assets/sprites
touch src/assets/sprites/sprite.svg
mkdir -p src/assets/images/sport
npm install
npm start 